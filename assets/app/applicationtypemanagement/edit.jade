            input#both(name="voteType", type='radio', value="both" required='',autocomplete="off", title="不能为空")
            label(for="both") #{i18n["both"]}
        .form-group.clearfix#approversGroup
            .input.btn.btn-empty#addApprover #{i18n["addApprover"]}
            .form-text.v-margin-nicer
              #approvers(name="approvers" type="tag")
      .form.col-md-6.no-margin
        .form-group.form-animate-text.required.v-margin-nicer
          input#name.form-text(name="name", type='text', required='',autocomplete="off")
          span.bar
          label #{i18n["appTypeName"]}
        .form-group.form-animate-text
          textarea.textarea.med#detail.form-text(name="detail", type='text', required='',autocomplete="off")
          span.bar
          label #{i18n["summary"]}
        .form-group.clearfix
          .checkbox3.checkbox-info.checkbox-inline.checkbox-check.checkbox-round.checkbox-light.checkbox-white.isMqttServer-checkbox
            input#remote.form-text(name="remote", type='checkbox', required='',autocomplete="off", title="不能为空",value="Y")
