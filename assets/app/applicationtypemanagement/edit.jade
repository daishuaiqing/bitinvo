.animated.fadeIn.sm-delay-1
  .navbar-header.breadcrumbs-cont
  .col-sm-12.form-panelbody
    form
      .form.col-md-6.no-margin
        p.error-info.hide #{i18n["error"]}
        input#id.form-text(name="id", type='hidden', required='',autocomplete="off")
        .form-group.clearfix
          label.form-group-label.col-sm-12 #{i18n["appTypeName"]}
          .radio3.radio-info.radio-inline.radio-check.radio-round.radio-light.radio-white.col-lg-3
            input#gun(name="type", type='radio', value="gun" required='',autocomplete="off", title="不能为空" checked)
            label(for="gun") #{i18n["getGun"]}
          .radio3.radio-info.radio-inline.radio-check.radio-round.radio-light.radio-white.col-lg-3
            input#bullet(name="type", type='radio', value="bullet" required='',autocomplete="off", title="不能为空")
            label(for="bullet") #{i18n["getBullet"]}
          .radio3.radio-info.radio-inline.radio-check.radio-round.radio-light.radio-white.col-lg-4
            input#emergency(name="type", type='radio', value="emergency" required='',autocomplete="off", title="不能为空")
            label(for="emergency") #{i18n["emergency"]}
          .radio3.radio-info.radio-inline.radio-check.radio-round.radio-light.radio-white.col-lg-3
            input#storageGun(name="type", type='radio', value="storageGun" required='',autocomplete="off", title="不能为空")
            label(for="storageGun") #{i18n["storageGun"]}
          .radio3.radio-info.radio-inline.radio-check.radio-round.radio-light.radio-white.col-lg-3
            input#storageBullet(name="type", type='radio', value="storageBullet" required='',autocomplete="off", title="不能为空")
            label(for="storageBullet") #{i18n["storageBullet"]}
          .radio3.radio-info.radio-inline.radio-check.radio-round.radio-light.radio-white.col-lg-3
            input#maintain(name="type", type='radio', value="maintain" required='',autocomplete="off", title="不能为空")
            label(for="maintain") #{i18n["maintain"]}
        .form-group.clearfix
          label.form-group-label.col-sm-12#approverOptionroup #{i18n["approverType"]}
          .radio3.radio-info.radio-inline.radio-check.radio-round.radio-light.radio-white.col-lg-5
            input#none(name="approverOption", type='radio', value="none" required='',autocomplete="off", title="不能为空" checked)
            label(for="none") #{i18n["none"]}
          .radio3.radio-info.radio-inline.radio-check.radio-round.radio-light.radio-white.col-lg-5
            input#single(name="approverOption", type='radio', value="single" required='',autocomplete="off", title="不能为空" checked)
            label(for="single") #{i18n["single"]}
          .radio3.radio-info.radio-inline.radio-check.radio-round.radio-light.radio-white.col-lg-5
              input#arbitary(name="approverOption", type='radio', value="arbitary" required='',autocomplete="off", title="不能为空")
              label(for="arbitary") #{i18n["arbitary"]}
        .form-group.clearfix#voteTypeGroup.hide
          label.form-group-label.col-sm-12 #{i18n["multiApprovalType"]}
          .radio3.radio-info.radio-inline.radio-check.radio-round.radio-light.radio-white.col-lg-5
            input#affirmative(name="voteType", type='radio', value="affirmative" required='',autocomplete="off", title="不能为空" checked)
            label(for="affirmative") #{i18n["affirmative"]}
          .radio3.radio-info.radio-inline.radio-check.radio-round.radio-light.radio-white.col-lg-5
            input#unanimous(name="voteType", type='radio', value="unanimous" required='',autocomplete="off", title="不能为空")
