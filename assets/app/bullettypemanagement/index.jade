.container-fluid.usermanangement-screen.screen
    .leftmenu-holder.animated.fadeInLeft
        .leftmenu
    .navbar.navbar-default.navbar-fixed-top.animated.fadeInDown
      .container.appm-comp-clock.comp-clock.text-center #{i18n['bulletManage']} / #{i18n['bulletTypeManage']}
      .appm-status-bar.text-left
      .appm-action-bar.text-right
    .row.right-major
      //.row.search-box.seperate-after this is search box
      //.row.tag-list.seperate-after this is tag
      .list-cont
        .actions-bar.seperate-after.animated.fadeIn.col-sm-12
          .pull-left
            button.btn.btn-empty.highlight.button-group__item(name="add")
              h4
                | #{i18n["addNewType"]}
                .glyphicon.glyphicon-check.h-margin-nicer
            button.btn.btn-empty.highlight.button-group__item(name="refresh")
              h4
                | #{i18nButton['refreshBtn']}
                .glyphicon.glyphicon-refresh.h-margin-nicer
            button.btn.btn-empty.highlight(name="search")
              h4
                | #{i18nButton['searchBtn']}
                .glyphicon.glyphicon-search.h-margin-nicer
          .pull-right.paging-btn-box
        .bullet-type-list.animated.fadeIn.md-delay-2
          //this is list
      .edit-cont
      .search-cont
