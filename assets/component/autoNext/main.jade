.panel.autoUpdateApp.col-md-offset-3.col-md-6.clearfix
  .pull-middle-center
    .leftModule.col-md-6.borderRight
      h1.text-center.mrgB30 #{i18n.cabinetOpenDoorSuccess}
      .btn.btn-empty.big.btn-fixed-len-300.highlight.mrgB60.completedBtn
        h1 #{i18n.completed}
      h1.text-center
        span.countdown 60
        span #{i18n.countdownTips}
    .rightModule.col-md-6
      h1.text-center.mrgB30 #{i18n.cabinetOpenDoorError}
      .btn.btn-empty.big.btn-fixed-len-300.keepBtn
        h1 #{i18n.exit}